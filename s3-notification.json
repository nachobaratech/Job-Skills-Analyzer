{
  "LambdaFunctionConfigurations": [
    {
      "Id": "TriggerLambdaOnUpload",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:223280412524:function:JobSkillsETLTrigger",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "processed/"
            },
            {
              "Name": "suffix",
              "Value": ".jsonl"
            }
          ]
        }
      }
    }
  ]
}
